/**/  int phase_a = 2;  int level_a = 3;  int phase_b = 4;  int level_b = 5;    int set_mode = 7;  void setup() {  //Motor A Setup  pinMode(phase_a, OUTPUT);  pinMode(level_a, OUTPUT);    //Motor B Setup  pinMode(phase_b, OUTPUT);  pinMode(level_b, OUTPUT);    //Mode  pinMode(set_mode, OUTPUT);    }void loop() {  //Setting Mode  digitalWrite(set_mode, HIGH);    //Motor A Motion  digitalWrite(phase_a, HIGH);  analogWrite(level_a, 200);    //Motor B Motion    digitalWrite(phase_b, HIGH);  analogWrite(level_b, 200);    delay(1000);}